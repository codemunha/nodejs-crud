{
  "[typescript]": {
    "editor.formatOnSave": true
  },
  "editor.codeActionsOnSave": {
    "source.fixAll": true,
    "source.fixAll.eslint": true
  },
  "editor.quickSuggestions": {
    "strings": true
  },
  "editor.tabSize": 2,
  "eslint.nodePath": "/node_modules",
  "favorites.resources": [
    "node_modules/ktb-core",
    "node_modules/ktb-core/src/components",
    "src\\views\\app\\task\\TaskList.vue",
    "src\\views\\app\\task\\ProductTaskList.vue",
    "src\\views\\app\\task\\ImportTaskList.vue",
    "src\\views\\common\\ProductPreview.vue"
  ],
  "javascript.format.insertSpaceBeforeFunctionParenthesis": true,
  "javascript.validate.enable": true,
  "sortJSON.contextMenu": {
    "sortJSON": false,
    "sortJSONAlphaNum": false,
    "sortJSONAlphaNumReverse": false,
    "sortJSONKeyLength": false,
    "sortJSONKeyLengthReverse": false,
    "sortJSONReverse": false,
    "sortJSONType": false,
    "sortJSONTypeReverse": false,
    "sortJSONValues": false,
    "sortJSONValuesReverse": false
  },
  "vetur.format.defaultFormatter.html": "js-beautify-html",
  "vetur.format.defaultFormatter.js": "vscode-typescript",
  "vetur.format.defaultFormatter.ts": "prettier",
  "vetur.validation.template": true,
  "vueStyle.formatOnSave": true,
  "vueStyle.ignoreFilesOnSave": [],
  "vueStyle.preset": {},
  "files.associations": {
    ".env.*": "env"
  },
  "css.styleSheets": [
    // "https://cdn.jsdelivr.net/npm/bootstrap@5.0.0-beta1/dist/css/bootstrap.min.css",
    "https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/css/bootstrap.min.css",
  ]
}